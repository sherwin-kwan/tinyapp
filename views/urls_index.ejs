<!DOCTYPE html>
<html lang="en">
<head>
  <%- include('./partials/head');  %> 
</head>
<body>
  <main style="margin: 1em;">
    <h3>My URLs</h3>
    <table class="table">
      <thead>
        <tr>
          <th scope="col">Short URL</th>
          <th scope="col">Long URL</th>
        </tr>
      </thead>
      <tbody>
        <% for(let url in urls) { %>
          <tr>
            <td><%= url %></td>
            <td><%= urls[url] %></td>
          </tr>
        <% } %>
      </tbody>
    </table>
  </main>
  <!-- Bootstrap JS -->
  <footer>
    <%- include('./partials/footer'); %>
  </footer>
  
</body>
</html>
